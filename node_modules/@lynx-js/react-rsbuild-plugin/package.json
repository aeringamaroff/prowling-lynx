{
  "name": "@lynx-js/react-rsbuild-plugin",
  "version": "0.9.0",
  "description": "A rsbuild plugin for ReactLynx",
  "keywords": [
    "rsbuild",
    "Lynx",
    "ReactLynx"
  ],
  "license": "Apache-2.0",
  "author": {
    "name": "Qingyu Wang",
    "email": "colinwang.0616@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "dependencies": {
    "@lynx-js/css-extract-webpack-plugin": "0.5.0",
    "@lynx-js/react-alias-rsbuild-plugin": "0.9.0",
    "@lynx-js/react-refresh-webpack-plugin": "0.3.1",
    "@lynx-js/react-webpack-plugin": "0.6.6",
    "@lynx-js/runtime-wrapper-webpack-plugin": "0.0.7",
    "@lynx-js/template-webpack-plugin": "0.6.3",
    "@lynx-js/web-webpack-plugin": "0.6.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.51.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rsbuild/core": "1.2.11",
    "@rsbuild/plugin-sass": "1.2.2",
    "@rsbuild/plugin-typed-css-modules": "1.0.2",
    "@rsbuild/plugin-webpack-swc": "1.0.10",
    "@rsbuild/webpack": "1.2.3",
    "tailwindcss": "^3.4.17",
    "type-fest": "^4.35.0",
    "typia": "7.6.4",
    "typia-rspack-plugin": "^1.0.1",
    "@lynx-js/react": "0.105.0",
    "@lynx-js/rspeedy": "0.8.2",
    "@lynx-js/vitest-setup": "0.0.0"
  },
  "peerDependencies": {
    "@lynx-js/react": "^0.103.0 || ^0.104.0 || ^0.105.0"
  },
  "peerDependenciesMeta": {
    "@lynx-js/react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "api-extractor": "api-extractor run --verbose",
    "build": "rslib build",
    "test": "pnpm -w run test --project rspeedy/react"
  }
}